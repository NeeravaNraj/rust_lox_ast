class Array {
    private arr;
    private length;

    init() {
        this.arr = [];
        this.length = 0;
    }

    public push(item) {
        this.arr[this.length] = item;
        this.length += 1;
    }

    public pop() {
        let item = this.arr[this.length];
        this.length -= 1;
        return item;
    }

    public display() {
        print this.arr;
    }

    public len() {
        return this.length;
    }

    public get(index) {
        return this.arr[index];
    }
}

let a = Array();
a.push(1);
a.push(2);
a.push(3);
a.display();
print a.len();
